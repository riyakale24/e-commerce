{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true
    },
    "include": ["src/**/*.tsx", "src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  